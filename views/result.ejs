<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Hamster Kombat Card Calculator - Results</title>
    <link rel="stylesheet" href="/assets/css/styles.css">
</head>
<body>
    <div class="container">
        <h1>Hello, <%= username %>!</h1>
        <div class="cards">
            <% cards.forEach(function(card) { %>
                <div class="card">
                    <h2><%= card.title %></h2>
                    <p>Level: <%= card.level %></p>
                    <p>Price: <%= card.price %></p>
                    <p>Additional Income: <%= card.additionalIncome %></p>
                </div>
            <% }); %>
        </div>
    </div>
</body>
</html>
